{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/home",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/brand/brand",
      "style": {
        "navigationBarTitleText": "品牌",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }
    }
  ],
  "tabBar": {
    "color": "#909399",
    "selectedColor": "#fdc204",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/home/home",
        "iconPath": "/static/tabar/home.png",
        "selectedIconPath": "/static/tabar/home_fill.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/brand/brand",
        "iconPath": "/static/tabar/event.png",
        "selectedIconPath": "/static/tabar/event_fill.png",
        "text": "品牌"
      },
      {
        "pagePath": "pages/my/my",
        "iconPath": "/static/tabar/my.png",
        "selectedIconPath": "/static/tabar/my_fill.png",
        "text": "我的"
      }
    ]
  },
  "subPackages": [
    {
      // 子包的根目录
      "root": "pagesMember",
      "pages": [
        {
          "path": "setting/setting",
          "style": {
            "navigationBarTitleText": "设置",
            "enablePullDownRefresh": false
          }
        },
        {
          "path": "myFriend/myFriend",
          "style": {
            "navigationBarTitleText": "我的好友",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "myFriend/friendDetail",
          "style": {
            "navigationBarTitleText": "好友详情",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9"
          }
        },
        {
          "path": "myWallet/myWallet",
          "style": {
            "navigationBarTitleText": "我的钱包",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "myWallet/applyForCash",
          "style": {
            "navigationBarTitleText": "余额提现",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "voucher/voucher",
          "style": {
            "navigationBarTitleText": "代金券",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "voucher/voucherDetail",
          "style": {
            "navigationBarTitleText": "代金券明细",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "myScore/myScore",
          "style": {
            "navigationBarTitleText": "我的积分",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "myScore/scoreProductDetail",
          "style": {
            "navigationBarTitleText": "商品详情",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "myScore/exchange",
          "style": {
            "navigationBarTitleText": "积分兑换",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "myScore/scoreDetail",
          "style": {
            "navigationBarTitleText": "积分明细",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "myScore/scoreOrder",
          "style": {
            "navigationBarTitleText": "积分订单",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "applyFor/applyFor",
          "style": {
            "navigationBarTitleText": "申请主理人",
            "enablePullDownRefresh": false,
            "navigationBarBackgroundColor": "#fff4b9",
            "navigationStyle": "custom"
          }
        },
        {
          "path": "suggestion/suggestion",
          "style": {
            "navigationBarTitleText": "意见反馈",
            "enablePullDownRefresh": false
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
